{
  "name": "karura_dex",
  "version": "0.0.1",
  "description": "karura decentralized exchange analysis",
  "scripts": {
    "7days": "ts-node ./scripts/7days_simple.ts",
    "24hours": "ts-node ./scripts/24hours_simple.ts",
    "7days-complex": "ts-node ./scripts/7days.ts",
    "24hours-complex": "ts-node ./scripts/24hours.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "karura",
    "kusama",
    "polkadot",
    "acala"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@acala-network/api": "^2.0.1",
    "@acala-network/types": "^2.0.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/testing": "^8.0.6",
    "@polkadot/api": "^5.4.1",
    "@polkadot/ts": "^0.4.4",
    "dotenv": "^8.2.0",
    "helmet": "^4.4.1",
    "mongoose": "^5.13.7",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
